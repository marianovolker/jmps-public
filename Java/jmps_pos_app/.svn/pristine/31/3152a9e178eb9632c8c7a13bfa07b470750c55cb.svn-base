<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.jmps.android.pos.AbDiariosEgresoActivity$PlaceholderFragment" >

    <EditText
        android:id="@+id/txtAbDiariosEgresoPatenteLetras"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="@string/patenteLetras"
        android:inputType="textCapCharacters" >

        <requestFocus />
    </EditText>

    <EditText
        android:id="@+id/txtAbDiariosEgresoPatenteNumeros"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/txtAbDiariosEgresoPatenteLetras"
        android:ems="10"
        android:hint="@string/patenteNumeros"
        android:inputType="number" />

    <Button
        android:id="@+id/btnAbDiariosEgresoCancelar"
        android:layout_width="@dimen/button_width"
        android:layout_height="wrap_content"
        android:layout_below="@id/txtAbDiariosEgresoPatenteNumeros"
        android:layout_marginTop="50dp"
        android:text="@android:string/cancel" />

    <Button
        android:id="@+id/btnAbDiariosEgresoGuardar"
        android:layout_width="@dimen/button_width"
        android:layout_height="wrap_content"
        android:layout_below="@id/txtAbDiariosEgresoPatenteNumeros"
        android:layout_marginTop="50dp"
        android:layout_toRightOf="@id/btnAbDiariosEgresoCancelar"
        android:text="@string/egresar" />

</RelativeLayout>